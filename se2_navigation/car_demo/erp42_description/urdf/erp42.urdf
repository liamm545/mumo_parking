  <!--XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX-OK-XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX -->
  <link name="base_link">
    <visual name="visual">
      <origin xyz="0 0 0" rpy="0 0 3.14" />
      <geometry>
        <mesh filename="package://erp42_description/meshes/erp42_structure.dae" scale="1 1 1"/>
      </geometry>
    </visual>

    <collision>
      <geometry>
      <!--<box size=" 1.0 1.0 0.2"/>-->
      <mesh filename="package://erp42_description/meshes/erp42_structure.dae" scale="1 1 1"/>
      </geometry>
         <origin xyz="0 0 -0.03" rpy=" 0 0 3.14" />
    </collision>
    
    <inertial>
      <mass value="160"/>
      <origin xyz="0 0 0" />
      <inertia ixx="20.43" ixy="0.584" ixz="-0.191" iyy="54.197" iyz="-0.2449" izz="72.2588"/>
    </inertial>
  </link>

  <link name="fl_axle">
    <inertial>
      <mass value="1"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.004" ixy="0.0" ixz="0.0" iyy="0.004" iyz="0.0" izz="0.004"/>
    </inertial>
  </link>

  <link name="fr_axle">
    <inertial>
      <mass value="1"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.004" ixy="0.0" ixz="0.0" iyy="0.004" iyz="0.0" izz="0.004"/>
    </inertial>
  </link>

  <link name="front_left_wheel">
    <inertial>
      <mass value="11"/> 
      <origin xyz="0 0 0"/> 
      <inertia ixx="0.58631238" ixy="0.0" ixz="0.0" iyy="0.33552910" iyz="0.0" izz="0.33552910"/>
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.28" length="0.15"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 1.5707"/> 
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5707"/> 
      <geometry>
        <mesh filename="package://erp42_description/meshes/caucho.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <gazebo>
      <mu1 value="0.72"/> 
      <mu2 value="0.72"/>
      <kp value="10000000.0" /> 
      <kd value="1.0" />
      <material>Gazebo/Grey</material> 
    </gazebo>
  </link>

  <link name="front_right_wheel">
    <inertial>
      <mass value="11"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.58631238" ixy="0.0" ixz="0.0" iyy="0.33552910" iyz="0.0" izz="0.33552910"/>
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.28" length="0.15"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 1.5707"/>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5707"/>
      <geometry>
        <mesh filename="package://erp42_description/meshes/caucho.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <gazebo>
      <mu1 value="0.72"/>
      <mu2 value="0.72"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>
  </link>

  <link name="rear_left_wheel">
    <inertial>
      <mass value="11"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.58631238" ixy="0.0" ixz="0.0" iyy="0.33552910" iyz="0.0" izz="0.33552910"/>
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.28" length="0.15"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 1.5707"/>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5707"/>
      <geometry>
        <mesh filename="package://erp42_description/meshes/caucho.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <gazebo>
      <mu1 value="0.72"/>
      <mu2 value="0.72"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>
  </link>

  <link name="rear_right_wheel">
    <inertial>
      <mass value="11"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.58631238" ixy="0.0" ixz="0.0" iyy="0.33552910" iyz="0.0" izz="0.33552910"/>
    </inertial>
    <collision>
      <geometry>
        <cylinder radius="0.28" length="0.15"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 1.5707"/>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 1.5707"/>
      <geometry>
        <mesh filename="package://erp42_description/meshes/caucho.dae" scale="1 1 1"/>
      </geometry>
    </visual>
    <gazebo>
      <mu1 value="0.72"/>
      <mu2 value="0.72"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>
  </link>

  <link name="steering_wheel">
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.14583300" ixy="0.0" ixz="0.0" iyy="0.14583300" iyz="0.0" izz="0.125"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://erp42_description/meshes/steer_wheel.dae" scale="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>

  <joint name="steering_joint" type="continuous">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="steering_wheel"/>
    <axis xyz="0 0 1"/>
    <limit lower="-7.85" upper="7.85" effort="10000000" velocity="1000000"/>
  </joint>

  <!-- Wheel joints -->
  <joint name="front_left_steer_joint" type="continuous">
    <parent link="base_link"/>
    <child link="fl_axle"/>
    <origin xyz="0.53 0.51 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.8727" upper="0.8727" effort="10000000" velocity="1000000"/>
  </joint>

  <joint name="front_right_steer_joint" type="continuous">
    <parent link="base_link"/>
    <child link="fr_axle"/>
    <origin xyz="0.53 -0.51 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.8727" upper="0.8727" effort="10000000" velocity="1000000"/>
  </joint>

  <joint name="front_left_wheel_joint" type="continuous">
    <parent link="fl_axle"/>
    <child link="front_left_wheel"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <joint name="front_right_wheel_joint" type="continuous">
    <parent link="fr_axle"/>
    <child link="front_right_wheel"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <joint name="rear_left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rear_left_wheel"/>
    <origin xyz="-0.49 0.51 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <joint name="rear_right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="rear_right_wheel"/>
    <origin xyz="-0.49 -0.51 0" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
  </joint>

  <gazebo reference="base_link_connection">
    <disableFixedJointLumping>true</disableFixedJointLumping>
  </gazebo>

